	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"syso": {
		"prefix": "syso",
		"body": [
			"System.out.println('$1');",
		],
		"description": "system.out.println"
	},
	"classp": {
		"prefix": "classp",
		"body":[
			"public class $TM_FILENAME_BASE {\n}"
		]
	},
	"comment":{
		"prefix": "comment",
		"body":[
			"$BLOCK_COMMENT_START*",
			"* @author luhha $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
			"* [描述]:$1",
			"$BLOCK_COMMENT_END"
		]
	}
}